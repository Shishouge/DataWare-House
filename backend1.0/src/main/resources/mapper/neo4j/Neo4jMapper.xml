<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.backend1.mapper.neo4j.Neo4jMapper">


    <resultMap id="actor" type="com.example.backend1.entity.neo4j.Actor">
        <result column="name" property="name" />
    </resultMap>
    <select id="getActor" resultType="com.example.backend1.entity.neo4j.Actor">
        MATCH (n:Actor) WHERE n.name="Patty McCormack" RETURN n.name as name
    </select>

</mapper>
